{
  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
